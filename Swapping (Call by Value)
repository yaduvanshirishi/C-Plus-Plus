#include <iostream>
using namespace std;

void swap(int&,int&);//Declaration

int main() {
	// your code goes here
	int a=10,b=20;
	cout <<"a = " <<a<<" &a = "<<&a<<endl<<endl;
	cout <<"b = " <<b<<" &b = "<<&b<<endl<<endl;
	swap(a,b);
	cout <<"a = " <<a<<" &a = "<<&a<<endl<<endl;
	cout <<"b = " <<b<<" &b = "<<&b<<endl<<endl;
	return 0;
}

void swap(int &x,int &y){
	int t;
	t=x;
	x=y;
	y=t;
}
