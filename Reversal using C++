/*
1-No codes for Stack
2-No initialization
3-clear interface for stack 
4-available in library
*/

#include <iostream>
#include <stack>
#include <string.h>
using namespace std;

int main() {
	// your code goes here
	char str[10]="ABCDE";
	stack<char> s;
	for(int i=0;i<strlen(str);i++)
		s.push(str[i]);
	cout<<"Reversed String ";
	while(!s.empty()){
		cout<<s.top();
		s.pop();
	}
	return 0;
}
